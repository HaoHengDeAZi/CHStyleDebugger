}

QPushButton#listWidgetBgButton QPushButton::pressed
{
    border: 0px;
    background-color: transparent;
}

QPushButton#listWidgetBgButton QPushButton#copyClassInfo
{
    background-image: url(:/Image/copy.png);
}

QPushButton#listWidgetBgButton QPushButton#copyClassInfo::hover
{
    background-image: url(:/Image/copy.png);
}

QPushButton#listWidgetBgButton QPushButton#copyClassInfo::pressed
{
    background-image: url(:/Image/copy.png);
}

QPushButton#listWidgetBgButton QPushButton::pressed
{
    border: 0px;
    background-color: transparent;
}

QPushButton#listWidgetBgButton QPushButton#menuButton
{
    background-image: url(:/Image/right.png);
}

QPushButton#listWidgetBgButton QPushButton#menuButton::hover
{
    background-image: url(:/Image/right.png);
}

QPushButton#listWidgetBgButton QPushButton#menuButton::pressed
{
    background-image: url(:/Image/right.png);
}

ObjectInfoMenu QWidget#wObjectTitle
{
    height: 26px;
    max-height: 26px;
    min-height: 26px;
    background-color: #A0A0A0;
}

ObjectInfoMenu QLabel#lblClassName, QLabel#lblObjectName
{
    background-color: transparent;
    color: #FFFFFF;
}

ObjectInfoMenu QLabel#lblObjectName
{
    max-width: 240px;
}

/*关于*/
About QLabel#lblAppIcon
{
    width: 183px;
